{
  "expo": {
    "scheme": "com.supabase",
    "name": "Thrive Productivity App",
    "slug": "Thrive",
    "version": "1.5.0",
    "orientation": "portrait",
    "icon": "assets/ThriveAppIcon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"

    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "buildNumber": "9",
      "bundleIdentifier": "com.superstarcoder.ThriveApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.superstarcoder.ThriveApp"
    },
    "web": {
      "favicon": "./assets/ThriveAppIcon.png"
    },
    "plugins": [
      "expo-splash-screen",
      "expo-font",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "6da1158c-9fa6-4ffe-948c-e6eada7c60af"
      }
    },
    "owner": "superstarcoder"
  }
}
